RewriteEngine On
Options +FollowSymlinks
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f

RewriteRule ^news/$ index.php [QSA,L]
RewriteRule ^mobile/([0-9]+)-([a-z0-9\-]+)/?$ 
RewriteRule ^post/category/(.*)$ wblog/category/$1 [R=301,L]
RewriteRule ^post/(.*)$ blog/post/$1 [R=301,L]